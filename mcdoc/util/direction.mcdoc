enum(string) Direction {
	Down = "down",
	Up = "up",
	North = "north",
	East = "east",
	South = "south",
	West = "west",
}

enum(byte) DirectionByte {
	Down = 0b,
	Up = 1b,
	North = 2b,
	South = 3b,
	West = 4b,
	East = 5b,
}

enum(int) DirectionInt {
	Down = 0,
	Up = 1,
	North = 2,
	South = 3,
	West = 4,
	East = 5,
}
